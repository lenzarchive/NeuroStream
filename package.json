{
  "name": "neurostream",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "prisma generate --schema=./prisma/development.prisma && node server.js",
    "build": "prisma generate --schema=./prisma/production.prisma && prisma db push --schema=./prisma/production.prisma --accept-data-loss && next build",
    "start": "node server.js",
    "lint": "next lint",
    "postinstall": "prisma generate --schema=./prisma/production.prisma"
  },
  "dependencies": {
    "@prisma/client": "^6.15.0",
    "bcryptjs": "^3.0.2",
    "jsonwebtoken": "^9.0.2",
    "next": ">=14.2.32",
    "next-auth": "^4.24.5",
    "prisma": "^6.15.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.7.5",
    "tailwindcss": "^3.4.17"
  },
  "devDependencies": {
    "@types/bcryptjs": "^3.0.0",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^24.3.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^9.35.0",
    "eslint-config-next": "15.5.2",
    "postcss": "^8.4.32",
    "postcss-import": "^16.0.0",
    "typescript": "^5.2.2"
  }
}
